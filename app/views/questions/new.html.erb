<h1> Create a New Question about Space cats </h1>

<%= flash[:notice] %>

<%= form_for @question do |f| %>
<div>
  <%= f.label :question_name %>
  <%= f.text_field :question_name %>
</div>

<div>
  <%= f.label :description %>
  <%= f.text_area :description %>
</div>

<div>
  <%= f.label :user_id %>
  <%= f.text_field :user_id %>
</div>

<div>
  <%= f.submit "Submit" %>
</div>

<% end %>
